---
import type { TimeLineEntryProps } from "../../data/types";
import TimeLineEntry from "./TimeLineEntry.astro";

type Props = {
  entries: TimeLineEntryProps[];
};
const { entries } = Astro.props;
---

<ul class="w-full sm:table sm:px-10 lg:px-20">
  {entries.map((entry) => <TimeLineEntry {...entry} />)}
</ul>
